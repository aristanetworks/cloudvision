{
  "name": "cloudvision-connector",
  "version": "3.4.15",
  "description": "A module to communicate with the CloudVision API server",
  "author": "extensions@arista.com",
  "homepage": "http://www.arista.com",
  "license": "MIT",
  "main": "lib/cloudvision-connector.js",
  "unpkg": "dist/cloudvision-connector.js",
  "module": "es/cloudvision-connector.js",
  "scripts": {
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/cloudvision-connector.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/cloudvision-connector.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/cloudvision-connector.min.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/cloudvision-connector.js",
    "build:try": "cross-env BABEL_ENV=es NODE_ENV=development INCLUDE_EXTERNAL=true rollup -c -o dist/cloudvision-connector-full.js",
    "build": "yarn run clean && yarn run build:es && yarn run build:commonjs && yarn run build:umd && yarn run build:umd:min && yarn run docs && yarn run docs:readme && yarn run docs:md",
    "check": "yarn run lint && yarn run flow && yarn run test",
    "clean": "rimraf dist lib coverage es docs",
    "docs": "documentation build src/** --document-exported --sort-order alpha --config documentation.yml -f html -o docs",
    "docs:md": "documentation build src/** --document-exported --sort-order alpha --config documentation-export.yml -f md -o docs/cloudvision-connector.md",
    "docs:readme": "documentation readme src/index.js --shallow --sort-order alpha --section=API",
    "flow": "flow check",
    "lint": "eslint *.js src/** test/**/*.js",
    "prepare": "yarn run lint && yarn run flow && yarn run test:cov && yarn run build",
    "test:clean": "rimraf .jest-cache",
    "test:cov": "rimraf coverage && yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "test": "cross-env BABEL_ENV=test NODE_ENV=production jest"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "keywords": [
    "arista",
    "cloud",
    "cloudvision-api",
    "cloudvision-connector",
    "cloudvision",
    "cvp",
    "net-db",
    "sdn",
    "switch"
  ],
  "dependencies": {
    "a-msgpack": "0.1.42",
    "base64-js": "1.3.0",
    "imurmurhash": "0.1.4",
    "jsbi": "2.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.4.5",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "cross-env": "5.2.0",
    "documentation": "12.1.1",
    "eslint": "5.16.0",
    "eslint-config-arista": "0.2.23",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "flow-bin": "0.102.0",
    "jest": "24.8.0",
    "js-yaml": "3.13.1",
    "mock-socket": "8.0.5",
    "rimraf": "2.6.3",
    "rollup": "1.12.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.1.1"
  }
}
