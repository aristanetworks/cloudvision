{
  "name": "cloudvision-grpc-web",
  "version": "4.8.0",
  "description": "A Grpc-Web client for CloudVision",
  "keywords": [
    "grpc",
    "grpc-web",
    "cloud",
    "cloudvision-api",
    "cloudvision",
    "arista",
    "cvp",
    "net-db",
    "sdn",
    "switch"
  ],
  "author": "extensions@arista.com",
  "homepage": "https://aristanetworks.github.io/cloudvision/modules/cloudvision_grpc_web.html",
  "license": "MIT",
  "main": "lib/cloudvision-grpc-web.js",
  "unpkg": "dist/cloudvision-grpc-web.js",
  "module": "es/cloudvision-grpc-web.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aristanetworks/cloudvision.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run docs && npm run docs:md && npm run build:types",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/cloudvision-grpc-web.js",
    "build:es": "cross-env NODE_ENV=es rollup -c -o es/cloudvision-grpc-web.js",
    "build:types": "rimraf typings && tsc --emitDeclarationOnly -p tsconfig-build.json -outDir ./typings",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/cloudvision-grpc-web.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/cloudvision-grpc-web.min.js",
    "check": "npm run lint && npm run type-check && npm run prettier-check",
    "generate": "rimraf generated && bash generate.sh",
    "clean": "rimraf dist lib coverage es docs typings",
    "docs": "typedoc --options typedoc-neo.json --plugin typedoc-neo-theme --theme ./node_modules/typedoc-neo-theme/bin/default --out docs/html",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --theme markdown --out docs/md --hideSources",
    "lint": "eslint --max-warnings 0 --ext .js,.ts *.js src/**/*.ts test/**/*.ts types/**/*.ts",
    "prepare": "npm run build",
    "prettier-check": "prettier -c *.js src/**/*.ts test/**/*.ts types/**/*.ts",
    "prettier-fix": "prettier --write *.js src/**/*.ts test/**/*.ts types/**/*.ts",
    "test": "cross-env NODE_ENV=production jest",
    "test:clean": "rimraf .jest-cache",
    "test:cov": "rimraf coverage && npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/aristanetworks/cloudvision/issues"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "0.14.0",
    "google-protobuf": "3.17.1",
    "rxjs": "6.6.7"
  },
  "devDependencies": {
    "@arista/prettier-config": "1.1.4",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "18.1.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "8.2.1",
    "@types/google-protobuf": "3.15.2",
    "@types/jest": "25.2.3",
    "cross-env": "7.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-arista-js": "1.1.36",
    "eslint-config-arista-ts": "1.4.6",
    "eslint-plugin-arista": "0.2.2",
    "eslint-plugin-import": "2.23.3",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.49.0",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "26.5.6",
    "ts-protoc-gen": "0.15.0",
    "tslib": "2.2.0",
    "typedoc": "0.19.2",
    "typedoc-neo-theme": "1.1.1",
    "typedoc-plugin-markdown": "3.1.1",
    "typescript": "4.2.4"
  }
}
