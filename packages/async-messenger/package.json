{
  "name": "async-messenger",
  "version": "1.0.0",
  "description": "An asynchronous message-passing API",
  "author": "extensions@arista.com",
  "homepage": "http://www.arista.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aristanetworks/cloudvision.git"
  },
  "license": "MIT",
  "keywords": [
    "arista"
  ],
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "typings"
  ],
  "main": "lib/async-messenger.js",
  "unpkg": "dist/cloudvision-connector.js",
  "module": "es/cloudvision-connector.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run docs && npm run docs:md && npm run build:types",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/async-messenger.js",
    "build:es": "cross-env NODE_ENV=es rollup -c -o es/async-messenger.js",
    "build:try": "cross-env NODE_ENV=development INCLUDE_EXTERNAL=true rollup -c -o dist/async-messenger-full.js",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig-build.json",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/async-messenger.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/async-messenger.min.js",
    "check": "npm run type-check && npm run prettier-check && npm run lint",
    "clean": "rimraf coverage dist docs es lib typings",
    "docs": "typedoc --toc index --out docs/html",
    "docs:md": "typedoc --theme markdown --out docs/md --hideSources",
    "lint": "eslint --max-warnings 0 --ext .js,.ts *.ts src/**/*.ts test/**/*.ts types/**/*.ts",
    "prepare": "npm run build",
    "prettier-check": "prettier -c *.js *.ts src/**/*.ts test/**/*.ts types/**/*.ts",
    "prettier-fix": "prettier --write *.js *.ts src/**/*.ts test/**/*.ts types/**/*.ts",
    "test": "cross-env NODE_ENV=production jest",
    "test:clean": "rimraf .jest-cache",
    "test:cov": "rimraf coverage && npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@arista/prettier-config": "1.1.3",
    "@rollup/plugin-commonjs": "12.0.0",
    "@rollup/plugin-node-resolve": "8.0.0",
    "@rollup/plugin-replace": "2.3.2",
    "@rollup/plugin-typescript": "4.1.2",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "3.12.4",
    "cloudvision-connector": "4.3.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-arista-js": "1.1.23",
    "eslint-config-arista-ts": "1.1.8",
    "eslint-plugin-arista": "0.1.32",
    "eslint-plugin-import": "2.21.2",
    "jest": "26.0.1",
    "js-yaml": "3.14.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rollup": "2.11.2",
    "rollup-plugin-terser": "5.3.0",
    "ts-jest": "26.0.0",
    "tslib": "2.0.0",
    "typedoc": "0.17.7",
    "typedoc-neo-theme": "1.0.8",
    "typedoc-plugin-markdown": "2.2.17",
    "typescript": "3.9.3"
  }
}
