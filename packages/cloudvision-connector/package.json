{
  "name": "cloudvision-connector",
  "version": "4.6.2",
  "description": "A module to communicate with the CloudVision API server",
  "author": "extensions@arista.com",
  "homepage": "https://aristanetworks.github.io/cloudvision/modules/cloudvision_connector.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/aristanetworks/cloudvision.git"
  },
  "license": "MIT",
  "main": "lib/cloudvision-connector.js",
  "unpkg": "dist/cloudvision-connector.js",
  "module": "es/cloudvision-connector.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run docs && npm run docs:md && npm run build:types",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/cloudvision-connector.js",
    "build:es": "cross-env NODE_ENV=es rollup -c -o es/cloudvision-connector.js",
    "build:try": "cross-env NODE_ENV=development INCLUDE_EXTERNAL=true rollup -c -o dist/cloudvision-connector-full.js",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig-build.json",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/cloudvision-connector.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/cloudvision-connector.min.js",
    "check": "npm run lint && npm run type-check && npm run prettier-check",
    "clean": "rimraf dist lib coverage es docs typings",
    "docs": "typedoc --options typedoc-neo.json --plugin typedoc-neo-theme --theme ./node_modules/typedoc-neo-theme/bin/default --out docs/html",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --theme markdown --out docs/md --hideSources",
    "lint": "eslint --max-warnings 0 --ext .js,.ts *.js src/**/*.ts test/**/*.ts types/**/*.ts",
    "prepare": "npm run build",
    "prettier-check": "prettier -c *.js src/**/*.ts test/**/*.ts types/**/*.ts",
    "prettier-fix": "prettier --write *.js src/**/*.ts test/**/*.ts types/**/*.ts",
    "test": "cross-env NODE_ENV=production jest",
    "test:clean": "rimraf .jest-cache",
    "test:cov": "rimraf coverage && npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "typings"
  ],
  "keywords": [
    "arista",
    "cloud",
    "cloudvision-api",
    "cloudvision-connector",
    "cloudvision",
    "cvp",
    "net-db",
    "sdn",
    "switch"
  ],
  "dependencies": {
    "a-msgpack": "4.6.2",
    "base64-js": "1.5.1",
    "jsbi": "3.1.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@arista/prettier-config": "1.1.4",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "6.1.0",
    "@types/base64-js": "1.3.0",
    "@types/imurmurhash": "0.1.1",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "3.12.4",
    "@types/uuid": "8.3.0",
    "cross-env": "7.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-arista-js": "1.1.35",
    "eslint-config-arista-ts": "1.4.4",
    "eslint-plugin-arista": "0.1.44",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "js-yaml": "3.14.0",
    "mock-socket": "9.0.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.34.2",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "26.4.4",
    "tslib": "2.0.3",
    "typedoc": "0.19.2",
    "typedoc-neo-theme": "1.0.10",
    "typedoc-plugin-markdown": "3.0.11",
    "typescript": "4.1.2"
  }
}
